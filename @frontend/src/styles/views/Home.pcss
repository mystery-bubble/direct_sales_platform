.view.home {
  @apply flex flex-col;
}

.search-box {
  @apply p-1 lg:p-2 bg-custom-white mb-2;

  border-radius: 8px;
}

.search-box > .inner {
  @apply flex flex-row justify-between items-center bg-transparent py-1.5 px-2 text-lg;

  border-radius: 8px;
}

.search-box > .inner.focus {
  @apply border border-custom-black;

  padding-top: calc( 0.375rem - 1px );
  padding-bottom: calc( 0.375rem - 1px );
  padding-left: calc( 0.5rem - 1px );
  padding-right: calc( 0.5rem - 1px );
}

.search-icon {
  @apply text-2xl
}

.search-input-field {
  @apply ml-2 focus:outline-none flex-grow bg-transparent;
}

.horizontal-types {
  @apply flex flex-row overflow-x-hidden justify-between items-center mb-2 2xl:hidden;
}

.horizontal-types > .arrow {
	@apply bg-custom-white p-3 text-2xl;

	border-radius: 8px;
}

.horizontal-types > .types-container {
	@apply overflow-hidden bg-custom-white text-center flex-grow py-2 px-2 mx-1.5 whitespace-nowrap;
	
	grid-auto-rows: auto;
	border-radius: 8px;
}

.horizontal-types > .types-container > .type-item {
	@apply w-1/2 inline-block mr-2 text-custom-black bg-custom-dark-trn-1 font-bold text-lg py-0.5;

	width: 32.25%;
	border-radius: 8px;
}

.horizontal-types > .types-container > .type-item.active {
	@apply bg-custom-black text-custom-white
}

.list.container {
  @apply grid sm:grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-3 py-5 px-6 bg-custom-white;

  border-radius: 12px;
}

.home.disable-mask {
  @apply w-screen h-screen fixed top-0 left-0;

  z-index: 1;
	background-color: rgba( 0, 0, 0, .7 )
}

.home.floating-group {
  @apply flex flex-row items-end fixed bottom-3 right-3 xl:hidden;

  z-index: 1;
}

.floating-button {
  @apply rounded-full bg-custom-black ml-2;

 width: 65px;
 height: 65px;
}

.floating-button > .button-icon {
  @apply relative text-4xl text-custom-white;

  top: 16.8px;
  left: 14.48px;
}

.floating-menu {
  @apply p-3 bg-custom-lighter;

  border-radius: 8px;
  height: 60vh;
  width: 50vw;
}

.float-expand-enter-active {
  transform-origin: bottom right;
	animation: floatExpand .2s;
}

.float-expand-leave-active {
  transform-origin: bottom right;
	animation: floatExpand .2s reverse;
}

.fade-enter-active, .fade-leave-active {
  transition: opacity .25s;
}

.fade-enter, .fade-leave-to {
  opacity: 0;
}

